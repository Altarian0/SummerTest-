        <TextBox x:Name="PasswordBox" HorizontalAlignment="Left" Height="25" Margin="234,269,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="364"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="347,322,0,0" VerticalAlignment="Top" Width="145" Click="Button_Click" Height="36"/>
